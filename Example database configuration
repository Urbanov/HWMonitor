In this example password always equals username.
Run this commands in psql:
----------------------------------------------------------------------
DROP DATABASE hwmonitor;
CREATE DATABASE hwmonitor;
\c hwmonitor;

CREATE TABLE users(
   username varchar(20) NOT NULL,
   password varchar(60) NOT NULL,
   enabled boolean NOT NULL DEFAULT FALSE,
   primary key(username)
);

CREATE TABLE companies(
   company_id SERIAL PRIMARY KEY,
   company_name varchar(20) NOT NULL,
   company_role varchar(20) NOT NULL
);

CREATE TABLE feeders(
   feeder_id SERIAL PRIMARY KEY,
   company_id integer NOT NULL,
   feeder_desc varchar(64) NOT NULL,
   FOREIGN KEY (company_id) REFERENCES companies (company_id)
);

CREATE TABLE feeders(
   feeder_id SERIAL PRIMARY KEY,
   company_id integer NOT NULL,
   feeder_desc varchar(64) NOT NULL,
   FOREIGN KEY (company_id) REFERENCES companies (company_id)
);

CREATE TABLE measurements(
   id SERIAL PRIMARY KEY,
   feeder_id integer NOT NULL,
   value integer NOT NULL,
   time timestamp NOT NULL,
   FOREIGN KEY (feeder_id) REFERENCES feeders (feeder_id)
);

create table user_roles (
  user_role_id SERIAL PRIMARY KEY,
  username varchar(20) NOT NULL,
  role varchar(20) NOT NULL,
  UNIQUE (username,role),
  FOREIGN KEY (username) REFERENCES users (username)
);

INSERT INTO users(username,password,enabled) VALUES ('jack','$2a$10$bd4Qn3nFAGmg3hdohmyZfuj9.ibNEkfFQlqdDlPMwA0ZCkbg7Arr.', true);
